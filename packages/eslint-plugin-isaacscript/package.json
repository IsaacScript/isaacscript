{
  "name": "eslint-plugin-isaacscript",
  "version": "4.0.0",
  "description": "An ESLint plugin that contains useful rules.",
  "keywords": [
    "eslint",
    "plugin",
    "isaacscript",
    "lint",
    "linting",
    "prettier",
    "rules",
    "enum",
    "enums",
    "comment",
    "comments",
    "format",
    "autoformat",
    "complete",
    "sentences"
  ],
  "homepage": "https://isaacscript.github.io/",
  "bugs": {
    "url": "https://github.com/IsaacScript/isaacscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacScript/isaacscript.git"
  },
  "license": "MIT",
  "author": "Zamiell",
  "type": "module",
  "main": "./dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "create-rule": "tsx ./scripts/createRule.ts",
    "generate": "tsx ./scripts/generate.ts",
    "lint": "tsx ./scripts/lint.ts",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/type-utils": "8.50.1",
    "@typescript-eslint/utils": "8.50.1",
    "typescript-eslint": "8.50.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@types/jest": "30.0.0",
    "@typescript-eslint/rule-tester": "8.50.1",
    "complete-common": "2.11.0",
    "complete-node": "16.0.0",
    "jest": "30.2.0",
    "prettier": "3.7.4"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0",
    "typescript": ">= 5.0.0"
  }
}

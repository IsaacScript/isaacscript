{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ],
      "options": {
        "command": "bash packages/eslint-config-isaacscript/build.sh"
      }
    },
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "options": {
        "command": "bash packages/eslint-config-isaacscript/lint.sh"
      }
    }
  },
  "tags": []
}
